<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.cyberpwn.react</groupId>
	<artifactId>ReactPlugin</artifactId>
	<version>2x</version>
	<name>ReactPlugin</name>
	<description>Smart Server Performance</description>
	<properties>
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>
		<react.libs>${project.basedir}/lib</react.libs>
		<development.server.path>X:\servers\dynamic\plugins</development.server.path>
	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.5.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>development</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-compiler-plugin</artifactId>
						<version>3.5.1</version>
						<configuration>
							<source>1.7</source>
							<target>1.7</target>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-jar-plugin</artifactId>
						<version>2.3.1</version>
						<configuration>
							<outputDirectory>${development.server.path}</outputDirectory>
							<finalName>React</finalName>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>production</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-compiler-plugin</artifactId>
						<version>3.5.1</version>
						<configuration>
							<source>1.7</source>
							<target>1.7</target>
						</configuration>
					</plugin>
					<plugin>
						<groupId>com.github.wvengen</groupId>
						<artifactId>proguard-maven-plugin</artifactId>
						<version>2.0.6</version>
						<dependencies>
							<dependency>
								<groupId>net.sf.proguard</groupId>
								<artifactId>proguard-base</artifactId>
								<version>4.10</version>
							</dependency>
						</dependencies>
						<executions>
							<execution>
								<phase>package</phase>
								<goals>
									<goal>proguard</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<proguardVersion>4.10</proguardVersion>
							<options>
								<option>-keep public class myapp.Main{public static void
									main(java.lang.String[]);}
								</option>
							</options>
							<libs>
								<lib>${java.home}/lib/rt.jar</lib>
								<lib>${java.home}/lib/jce.jar</lib>
								<lib>${react.libs}/Craftbukkit-1.8.8.jar</lib>
								<lib>${react.libs}/Craftbukkit-1.9.2.jar</lib>
								<lib>${react.libs}/Craftbukkit-1.9.4.jar</lib>
								<lib>${react.libs}/Craftbukkit-1.10.jar</lib>
								<lib>${react.libs}/Spigot-1.8.8.jar</lib>
								<lib>${react.libs}/Paper-1.8.8.jar</lib>
								<lib>${react.libs}/WorldBorder.jar</lib>
								<lib>${react.libs}/ProtocolLib.jar</lib>
								<lib>${react.libs}/PlaceHolderAPI.jar</lib>
							</libs>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
	<repositories>

	</repositories>
	<dependencies>
		<dependency>
			<groupId>bukkit.org</groupId>
			<artifactId>craftbukkit-1.8.8</artifactId>
			<scope>system</scope>
			<systemPath>${server.jars}/Craftbukkit-1.8.8.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>spigot.org</groupId>
			<artifactId>spigot-1.8.8</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Spigot-1.8.8.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>paper</groupId>
			<artifactId>paper-1.8.8</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Paper-1.8.8.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>protocollib</groupId>
			<artifactId>protocollib</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/ProtocolLib.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>placeholderapi</groupId>
			<artifactId>placeholderapi</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/PlaceHolderAPI.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>worldborder</groupId>
			<artifactId>worldborder</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/WorldBorder.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>bukkit.org</groupId>
			<artifactId>craftbukkit-1.8.8</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Craftbukkit-1.8.8.jar</systemPath>
			<version>1.8.8</version>
		</dependency>
		<dependency>
			<groupId>bukkit.org</groupId>
			<artifactId>craftbukkit-1.9.2</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Craftbukkit-1.9.2.jar</systemPath>
			<version>1.9.2</version>
		</dependency>
		<dependency>
			<groupId>bukkit.org</groupId>
			<artifactId>craftbukkit-1.9.4</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Craftbukkit-1.9.4.jar</systemPath>
			<version>1.9.4</version>
		</dependency>
		<dependency>
			<groupId>bukkit.org</groupId>
			<artifactId>craftbukkit-1.10</artifactId>
			<scope>system</scope>
			<systemPath>${react.libs}/Craftbukkit-1.10.jar</systemPath>
			<version>1.10</version>
		</dependency>
	</dependencies>
</project>